class Employee :
    def __init__(self):
        print("This is the intialised")
    def empname(self,name,age):
        self.name = name
        self.age = age
        print("Employee name is {} and age is {}".format(self.name,self.age))

emp1 = Employee()
emp2 = Employee()
emp1.empname("Niranjan","29")
emp2.empname("Nithin","29")
